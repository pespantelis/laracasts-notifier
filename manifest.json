{
  "manifest_version": 2,
  "name": "Laracasts Notifier",
  "description": "Get a notification when laracasts.com publishes a new lesson.",
  "version": "1.0",
  "permissions": [
    "https://laracasts.com/lessons",
    "storage",
    "tabs"
  ],
  "icons": {
    "48": "assets/icon_48.png"
  },
  "background": {
    "scripts": ["assets/jquery.js", "background.js"]
  },
  "browser_action": {
    "default_icon": {
      "19": "assets/icon_19.png"
    }
  }
}
